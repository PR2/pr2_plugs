  <!-- head controller action -->
  <rosparam command="load" file="$(find pr2_plugs_executive)/config/pr2_plugs_head_controllers.yaml"/>
  <group ns="head_traj_controller"> 
     <node name="joint_trajectory_action_node" pkg="joint_trajectory_action" type="joint_trajectory_action" /> 
  </group>
</launch>
